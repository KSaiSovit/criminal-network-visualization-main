[
	{
		"selector": "node",
		"style": {
			"text-wrap": "wrap",
			"label": "data(label)",
			"font-size": "0.6em",
			"background-color": "grey",
			"width": "25vh",
			"height": "25vh",
			"text-wrap": "wrap"
		}
	},
	{
		"selector": "edge",
		"style": {
			"line-color": "grey",
			"curve-style": "bezier",
			"control-point-step-size": "10",
			"target-arrow-shape": "vee",
			"target-arrow-color": "grey",
			"width": "0.5px",
			"arrow-scale": "0.5",
			"label": ""
		}
	},
	{
		"selector": "[?expandable]",
